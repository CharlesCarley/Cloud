    <SolidColorBrush
        x:Key="LabelAccent"
        Color="#FF6FADD9" />
    <SolidColorBrush
        x:Key="LabelEmphasisStrong"
        Color="#FF123551" />
    <SolidColorBrush
        x:Key="LabelEmphasisNormal"
