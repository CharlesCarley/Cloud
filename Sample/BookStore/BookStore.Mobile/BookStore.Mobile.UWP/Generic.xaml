    <SolidColorBrush
        x:Key="LabelEmphasisWeak"
        Color="#FFBEC9D2" />
    <SolidColorBrush
        x:Key="LabelEmphasisAccentStrong"
        Color="#FF056CB7" />
    <SolidColorBrush
        x:Key="LabelEmphasisAccentNormal"
        Color="#FF5BA0D3" />
    <SolidColorBrush
        x:Key="LabelEmphasisAccentWeak"
        Color="#FFB9DAF2" />
    <SolidColorBrush
        x:Key="ControlBackground"
        Color="#FFFFFFFF" />
    <SolidColorBrush
        x:Key="ControlText"
        Color="#FF556069" />
    <SolidColorBrush
        x:Key="ControlBorder"
        Color="#FFC9D4DC" />
    <SolidColorBrush
        x:Key="HoverControlBackground"
        Color="#FFFFFFFF" />
    <SolidColorBrush
        x:Key="HoverControlText"
        Color="#FF556069" />
    <SolidColorBrush
        x:Key="HoverControlBorder"
        Color="#FF6FADD9" />
    <SolidColorBrush
