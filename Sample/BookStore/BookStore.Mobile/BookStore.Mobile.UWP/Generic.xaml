        Color="#FFF0F0F0" />
    <SolidColorBrush
        x:Key="PressedControlText"
        Color="#FF123551" />
    <SolidColorBrush
        x:Key="PressedControlBorder"
        Color="#FF6FADD9" />
    <SolidColorBrush
        x:Key="FocusedControlBackground"
        Color="#FFF0F0F0" />
    <SolidColorBrush
        x:Key="FocusedControlText"
